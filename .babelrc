{
  "env": {
    "production": {
      "presets": ["env", "stage-2"],
      "plugins": [
        "transform-es2015-spread",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals"
      ]
    },
    "development": {
      "presets": ["env", "stage-2"],
      "plugins": [
        "transform-es2015-spread",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals"
      ]
    },
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [
        "rewire",
        "transform-es2015-spread",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals"
      ]
    }
  }
}
