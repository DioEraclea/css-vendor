{
  "name": "css-vendor",
  "description": "CSS vendor prefix detection and property feature testing.",
  "version": "0.3.7",
  "author": {
    "name": "Oleg Slobodskoi",
    "email": "oleg008@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cssinjs/css-vendor.git"
  },
  "keywords": [
    "css",
    "vendor",
    "feature",
    "test",
    "prefix",
    "cssinjs",
    "jss",
    "css-in-js"
  ],
  "engines": {},
  "scripts": {
    "all": "npm run lint && npm run build",
    "build": "npm run clean && npm run build:lib && npm run build:max && npm run build:min",
    "build:lib": "babel src --out-dir lib",
    "build:max": "cross-env NODE_ENV=development webpack src/index.js dist/css-vendor.js",
    "build:min": "cross-env NODE_ENV=production webpack src/index.js dist/css-vendor.min.js",
    "clean": "rimraf ./lib/*",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "test": "opener ./test/index.html",
    "prepublish": "npm run all"
  },
  "license": "MIT",
  "main": "./lib/index",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-jss": "^2.4.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "jss": "^5.4.0",
    "opener": "^1.4.1",
    "pre-commit": "^1.1.3",
    "qunitjs": "2.0.1",
    "rimraf": "^2.5.1",
    "webpack": "^2.0.7-beta"
  },
  "dependencies": {
    "is-in-browser": "^1.0.2"
  },
  "lint-staged": {
    "./src/*.js": [
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
